    "VERSE": "prophets:joshua:7:4",
    "HEBREW": "\u05d5\u05b7\u05d9\u05bc\u05b7\u05e2\u05b2\u05dc\u05a4\u05d5\u05bc \u05de\u05b4\u05df\u05be\u05d4\u05b8\u05e2\u05b8\u05dd\u0599 \u05e9\u05c1\u05b8\u0594\u05de\u05bc\u05b8\u05d4 \u05db\u05bc\u05b4\u05e9\u05c1\u05b0\u05dc\u05b9\u05a5\u05e9\u05c1\u05b6\u05ea \u05d0\u05b2\u05dc\u05b8\u05e4\u05b4\u0596\u05d9\u05dd \u05d0\u05b4\u0591\u05d9\u05e9\u05c1 \u05d5\u05b7\u05d9\u05bc\u05b8\u05e0\u05bb\u0595\u05e1\u05d5\u05bc \u05dc\u05b4\u05e4\u05b0\u05e0\u05b5\u0596\u05d9 \u05d0\u05b7\u05e0\u05b0\u05e9\u05c1\u05b5\u05a5\u05d9 \u05d4\u05b8\u05e2\u05b8\u05bd\u05d9\u05c3",
