    "VERSE": "prophets:jeremiah:36:15",
    "HEBREW": "\u05d5\u05b7\u05d9\u05bc\u05b9\u05d0\u05de\u05b0\u05e8\u05a3\u05d5\u05bc \u05d0\u05b5\u05dc\u05b8\u0594\u05d9\u05d5 \u05e9\u05c1\u05b5\u05a3\u05d1 \u05e0\u05b8\u0594\u05d0 \u05d5\u05bc\u05e7\u05b0\u05e8\u05b8\u05d0\u05b6\u0596\u05e0\u05bc\u05b8\u05d4 \u05d1\u05bc\u05b0\u05d0\u05b8\u05d6\u05b0\u05e0\u05b5\u0591\u05d9\u05e0\u05d5\u05bc \u05d5\u05b7\u05d9\u05bc\u05b4\u05e7\u05b0\u05e8\u05b8\u05a5\u05d0 \u05d1\u05b8\u05e8\u0596\u05d5\u05bc\u05da\u05b0 \u05d1\u05bc\u05b0\u05d0\u05b8\u05d6\u05b0\u05e0\u05b5\u05d9\u05d4\u05b6\u05bd\u05dd\u05c3",
