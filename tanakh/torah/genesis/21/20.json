    "VERSE": "torah:genesis:21:20",
    "HEBREW": "\u05d5\u05b7\u05d9\u05b0\u05d4\u05b4\u05a7\u05d9 \u05d0\u05b1\u05dc\u05b9\u05d4\u05b4\u059b\u05d9\u05dd \u05d0\u05b6\u05ea\u05be\u05d4\u05b7\u05e0\u05bc\u05b7\u0596\u05e2\u05b7\u05e8 \u05d5\u05b7\u05d9\u05bc\u05b4\u05d2\u05b0\u05d3\u05bc\u05b8\u0591\u05dc \u05d5\u05b7\u05d9\u05bc\u05b5\u0599\u05e9\u05c1\u05b6\u05d1\u0599 \u05d1\u05bc\u05b7\u05de\u05bc\u05b4\u05d3\u05b0\u05d1\u05bc\u05b8\u0594\u05e8 \u05d5\u05b7\u05d9\u05b0\u05d4\u05b4\u0596\u05d9 \u05e8\u05b9\u05d1\u05b6\u05a5\u05d4 \u05e7\u05b7\u05e9\u05c1\u05bc\u05b8\u05bd\u05ea\u05c3",
