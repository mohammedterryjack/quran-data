    "ARABIC": "\u0625\u0650\u0646\u0652 \u0647\u064f\u0648\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0648\u064e\u062d\u0652\u0649\u064c \u064a\u064f\u0648\u062d\u064e\u0649\u0670",
