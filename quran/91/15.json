    "ARABIC": "\u0648\u064e\u0644\u064e\u0627 \u064a\u064e\u062e\u064e\u0627\u0641\u064f \u0639\u064f\u0642\u0652\u0628\u064e\u0670\u0647\u064e\u0627",
    "ENGLISH": {
        "TRANSLATION_0": "He does not fear the consequence.\n",
        "TRANSLATION_1": "He has no fear of its sequel.\n",
