    "VERSE": "writings:esther:3:6",
    "HEBREW": "\u05d5\u05b7\u05d9\u05bc\u05b4\u05a3\u05d1\u05b6\u05d6 \u05d1\u05bc\u05b0\u05e2\u05b5\u05d9\u05e0\u05b8\u0597\u05d9\u05d5 \u05dc\u05b4\u05e9\u05c1\u05b0\u05dc\u05b9\u05a4\u05d7 \u05d9\u05b8\u05d3\u0599 \u05d1\u05bc\u05b0\u05de\u05b8\u05e8\u05b0\u05d3\u05bc\u05b3\u05db\u05b7\u05a3\u05d9 \u05dc\u05b0\u05d1\u05b7\u05d3\u05bc\u0594\u05d5\u05b9 \u05db\u05bc\u05b4\u05bd\u05d9\u05be\u05d4\u05b4\u05d2\u05bc\u05b4\u05a5\u05d9\u05d3\u05d5\u05bc \u05dc\u0596\u05d5\u05b9 \u05d0\u05b6\u05ea\u05be\u05e2\u05b7\u05a3\u05dd \u05de\u05b8\u05e8\u05b0\u05d3\u05bc\u05b3\u05db\u05b8\u0591\u05d9 \u05d5\u05b7\u05d9\u05b0\u05d1\u05b7\u05e7\u05bc\u05b5\u05a3\u05e9\u05c1 \u05d4\u05b8\u05de\u05b8\u0597\u05df \u05dc\u05b0\u05d4\u05b7\u05e9\u05c1\u05b0\u05de\u05b4\u05a7\u05d9\u05d3 \u05d0\u05b6\u05ea\u05be\u05db\u05bc\u05b8\u05dc\u05be\u05d4\u05b7\u05d9\u05bc\u05b0\u05d4\u05d5\u05bc\u05d3\u05b4\u059b\u05d9\u05dd \u05d0\u05b2\u05e9\u05c1\u05b6\u059b\u05e8 \u05d1\u05bc\u05b0\u05db\u05b8\u05dc\u05be\u05de\u05b7\u05dc\u05b0\u05db\u05a5\u05d5\u05bc\u05ea \u05d0\u05b2\u05d7\u05b7\u05e9\u05c1\u05b0\u05d5\u05b5\u05e8\u0596\u05d5\u05b9\u05e9\u05c1 \u05e2\u05b7\u05a5\u05dd \u05de\u05b8\u05e8\u05b0\u05d3\u05bc\u05b3\u05db\u05b8\u05bd\u05d9\u05c3",
