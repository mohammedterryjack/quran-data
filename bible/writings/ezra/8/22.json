    "VERSE": "writings:ezra:8:22",
    "HEBREW": "\u05db\u05bc\u05b4\u05a3\u05d9 \u05d1\u05b9\u0597\u05e9\u05c1\u05b0\u05ea\u05bc\u05b4\u05d9 \u05dc\u05b4\u05e9\u05c1\u05b0\u05d0\u05a4\u05d5\u05b9\u05dc \u05de\u05b4\u05df\u05be\u05d4\u05b7\u05de\u05bc\u05b6\u0599\u05dc\u05b6\u05da\u05b0\u0599 \u05d7\u05b7\u05a3\u05d9\u05b4\u05dc \u05d5\u05bc\u05e4\u05b8\u05e8\u05b8\u05e9\u05c1\u05b4\u0594\u05d9\u05dd \u05dc\u05b0\u05e2\u05b8\u05d6\u05b0\u05e8\u05b5\u05a5\u05e0\u05d5\u05bc \u05de\u05b5\u05d0\u05d5\u05b9\u05d9\u05b5\u0596\u05d1 \u05d1\u05bc\u05b7\u05d3\u05bc\u05b8\u0591\u05e8\u05b6\u05da\u05b0 \u05db\u05bc\u05b4\u05bd\u05d9\u05be\u05d0\u05b8\u05de\u05b7\u05a8\u05e8\u05b0\u05e0\u05d5\u05bc \u05dc\u05b7\u05de\u05bc\u05b6\u059c\u05dc\u05b6\u05da\u05b0 \u05dc\u05b5\u05d0\u05de\u05b9\u0597\u05e8 \u05d9\u05b7\u05d3\u05be\u05d0\u05b1\u05dc\u05b9\u05d4\u05b5\u05a4\u05d9\u05e0\u05d5\u05bc \u05e2\u05b7\u05dc\u05be\u05db\u05bc\u05b8\u05dc\u05be\u05de\u05b0\u05d1\u05b7\u05e7\u05b0\u05e9\u05c1\u05b8\u05d9\u05d5\u0599 \u05dc\u05b0\u05d8\u05d5\u05b9\u05d1\u05b8\u0594\u05d4 \u05d5\u05b0\u05e2\u05bb\u05d6\u05bc\u05a3\u05d5\u05b9 \u05d5\u05b0\u05d0\u05b7\u05e4\u05bc\u0594\u05d5\u05b9 \u05e2\u05b7\u0596\u05dc \u05db\u05bc\u05b8\u05dc\u05be\u05e2\u05b9\u05d6\u05b0\u05d1\u05b8\u05bd\u05d9\u05d5\u05c3",
