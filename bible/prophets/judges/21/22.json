    "VERSE": "prophets:judges:21:22",
    "HEBREW": "\u05d5\u05b0\u05d4\u05b8\u05d9\u05b8\u05a1\u05d4 \u05db\u05bc\u05b4\u05bd\u05d9\u05be\u05d9\u05b8\u05d1\u05b9\u05a3\u05d0\u05d5\u05bc \u05d0\u05b2\u05d1\u05d5\u05b9\u05ea\u05b8\u05dd\u05a9 \u05d0\u05a8\u05d5\u05b9 \u05d0\u05b2\u05d7\u05b5\u05d9\u05d4\u05b6\u059c\u05dd \u05dc\u05e8\u05d5\u05d1 [\u05dc\u05b8\u05e8\u05b4\u05a3\u05d9\u05d1 \u05c0] \u05d0\u05b5\u05dc\u05b5\u0597\u05d9\u05e0\u05d5\u05bc \u05d5\u05b0\u05d0\u05b8\u05de\u05b7\u05a4\u05e8\u05b0\u05e0\u05d5\u05bc \u05d0\u05b2\u05dc\u05b5\u05d9\u05d4\u05b6\u05dd\u0599 \u05d7\u05b8\u05e0\u05bc\u05a3\u05d5\u05bc\u05e0\u05d5\u05bc \u05d0\u05d5\u05b9\u05ea\u05b8\u0594\u05dd \u05db\u05bc\u05b4\u05a3\u05d9 \u05dc\u05b9\u05a5\u05d0 \u05dc\u05b8\u05e7\u05b7\u059b\u05d7\u05b0\u05e0\u05d5\u05bc \u05d0\u05b4\u05a5\u05d9\u05e9\u05c1 \u05d0\u05b4\u05e9\u05c1\u05b0\u05ea\u05bc\u0596\u05d5\u05b9 \u05d1\u05bc\u05b7\u05de\u05bc\u05b4\u05dc\u05b0\u05d7\u05b8\u05de\u05b8\u0591\u05d4 \u05db\u05bc\u05b4\u05a3\u05d9 \u05dc\u05b9\u05a5\u05d0 \u05d0\u05b7\u05ea\u05bc\u05b6\u059b\u05dd \u05e0\u05b0\u05ea\u05b7\u05ea\u05bc\u05b6\u05a5\u05dd \u05dc\u05b8\u05d4\u05b6\u0596\u05dd \u05db\u05bc\u05b8\u05e2\u05b5\u05a5\u05ea \u05ea\u05bc\u05b6\u05d0\u05b0\u05e9\u05c1\u05b8\u05bd\u05de\u05d5\u05bc\u05c3 (\u05e1)",
