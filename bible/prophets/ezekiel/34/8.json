    "VERSE": "prophets:ezekiel:34:8",
    "HEBREW": "\u05d7\u05b7\u05d9\u05be\u05d0\u05b8\u059c\u05e0\u05b4\u05d9 \u05e0\u05b0\u05d0\u05bb\u05a3\u05dd \u05c0 \u05d0\u05b2\u05d3\u05b9\u05e0\u05b8\u05a3\u05d9 \u05d9\u05b0\u05d4\u05d5\u05b4\u0597\u05d4 \u05d0\u05b4\u05dd\u05be\u05dc\u05b9\u05a3\u05d0 \u05d9\u05b7\u05a3\u05e2\u05b7\u05df \u05d4\u05b1\u05d9\u05bd\u05d5\u05b9\u05ea\u05be\u05e6\u05b9\u05d0\u05e0\u05b4\u05a3\u05d9 \u05c0 \u05dc\u05b8\u05d1\u05b7\u05a1\u05d6 \u05d5\u05b7\u05ea\u05bc\u05b4\u05bd\u05d4\u05b0\u05d9\u05b6\u05d9\u05e0\u05b8\u05d4\u05a9 \u05e6\u05b9\u05d0\u05e0\u05b4\u05a8\u05d9 \u05dc\u05b0\u05d0\u05b8\u05db\u05b0\u05dc\u05b8\u059c\u05d4 \u05dc\u05b0\u05db\u05b8\u05dc\u05be\u05d7\u05b7\u05d9\u05bc\u05b7\u05a4\u05ea \u05d4\u05b7\u05e9\u05c2\u05bc\u05b8\u05d3\u05b6\u05d4\u0599 \u05de\u05b5\u05d0\u05b5\u05a3\u05d9\u05df \u05e8\u05b9\u05e2\u05b6\u0594\u05d4 \u05d5\u05b0\u05dc\u05b9\u05bd\u05d0\u05be\u05d3\u05b8\u05e8\u05b0\u05e9\u05c1\u05a5\u05d5\u05bc \u05e8\u05b9\u05e2\u05b7\u0596\u05d9 \u05d0\u05b6\u05ea\u05be\u05e6\u05b9\u05d0\u05e0\u05b4\u0591\u05d9 \u05d5\u05b7\u05d9\u05bc\u05b4\u05e8\u05b0\u05e2\u05a4\u05d5\u05bc \u05d4\u05b8\u05bd\u05e8\u05b9\u05e2\u05b4\u05d9\u05dd\u0599 \u05d0\u05d5\u05b9\u05ea\u05b8\u0594\u05dd \u05d5\u05b0\u05d0\u05b6\u05ea\u05be\u05e6\u05b9\u05d0\u05e0\u05b4\u0596\u05d9 \u05dc\u05b9\u05a5\u05d0 \u05e8\u05b8\u05e2\u05bd\u05d5\u05bc\u05c3 (\u05e1)",
